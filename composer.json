{
    "name": "easyblue/rules-engine",
    "description": "Provides tools for rules engine pattern",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "symfony",
        "rules",
        "rules engine"
    ],
    "authors": [
        {
            "name": "Stello",
            "email": "dev@stello.eu"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/string": "^7.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Easyblue\\RulesEngine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Easyblue\\RulesEngine\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "require": "^7.0"
        }
    }
}
